<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivitymedical1">

  <Button
      android:id="@+id/buttonmedical1"
      android:layout_width="300dp"
      android:layout_height="wrap_content"
      android:background="@drawable/registerbtn"
      android:drawableRight="@drawable/ic_baseline_arrow_forward_ios_24"
      android:gravity="left|center_vertical"
      android:paddingLeft="10dp"
      android:text="GO TO EMERGENCY CONTACTS"
      android:textColor="@color/white"
      android:textSize="18sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.405"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.629" />

  <Button
      android:id="@+id/buttonmedical2"
      android:layout_width="300dp"
      android:layout_height="wrap_content"
      android:background="@drawable/registerbtn"
      android:drawableRight="@drawable/ic_baseline_arrow_forward_ios_24"
      android:gravity="left|center_vertical"
      android:paddingLeft="10dp"
      android:text="LOCATION AND HOSPITALS"
      android:textColor="@color/white"
      android:textSize="18sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.405"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.778" />

  <Button
      android:id="@+id/call"
      android:layout_width="200dp"
      android:layout_height="wrap_content"
      android:background="@drawable/registerbtn"
      android:drawableRight="@drawable/ic_baseline_arrow_forward_ios_24"
      android:text="CALL HOSPITALS "
      android:textColor="@color/white"
      android:textSize="20sp"
      android:textStyle="normal"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.213"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.919" />

  <ImageView
      android:id="@+id/imageView7"
      android:layout_width="410dp"
      android:layout_height="357dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/olla" />

</androidx.constraintlayout.widget.ConstraintLayout>